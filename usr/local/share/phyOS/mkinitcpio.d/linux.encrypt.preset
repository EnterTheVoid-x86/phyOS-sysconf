# mkinitcpio preset file for the 'linux' package on archiso

PRESETS=('default' 'safegraphics')

ALL_kver='/boot/vmlinuz-linux'
ALL_config='/usr/local/share/phyOS/mkinitcpio.d/mkinitcpio-encrypt.conf'
default_image="/boot/initramfs-linux.img"

safegraphics_image="/boot/initramfs-linux-safegraphics.img"
safegraphics_options="-S autodetect"
